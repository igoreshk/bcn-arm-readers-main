{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "plugins": [
        "react",
        "prettier",
        "jest",
        "promise",
        "more",
        "import"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "settings": {
        "react": {
            "version": "17.0.2"
        }
    },
    "rules": {
        "quotes": [2, "single"],
        "prettier/prettier": 2,
        "no-async-promise-executor": 2,
        "no-await-in-loop": 2,
        "no-console": [
            "error",
            {
                "allow": [
                    "warn",
                    "error"
                ]
            }
        ],
        "no-extra-parens": [
            2,
            "functions"
        ],
        "no-inner-declarations": [
            2,
            "both"
        ],
        "no-invalid-regexp": [
            2,
            {
                "allowConstructorFlags": [
                    "u",
                    "y"
                ]
            }
        ],
        "no-template-curly-in-string": 2,
        "require-atomic-updates": 2,
        "accessor-pairs": [
            2,
            {
                "getWithoutSet": true,
                "setWithoutGet": true
            }
        ],
        "array-callback-return": 2,
        "curly": 2,
        "dot-notation": 2,
        "eqeqeq": [
            2,
            "smart"
        ],
        "no-global-assign": 2,
        "no-magic-numbers": [
            2,
            {
                "ignore": [
                    0,
                    1,
                    -1
                ],
                "ignoreArrayIndexes": true,
                "enforceConst": true,
                "detectObjects": false
            }
        ],
        "no-multi-spaces": 2,
        "require-await": 2,
        "no-trailing-spaces": 2,
        "semi": 2,
        "no-var": 2,
        "complexity": [2, 10],
        "max-statements": [2, 20],
        "max-nested-callbacks": [2, 4],
        "max-params": [2, 5],
        "max-depth": [2, 2],
        "no-restricted-syntax": [2, "ForInStatement", "SequenceExpression"],
        "no-caller": 2,
        "no-else-return": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-implicit-coercion": [2, {"allow": ["!!"]}],
        "no-lone-blocks": 2,
        "no-param-reassign": 2,
        "no-proto": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-useless-call": 2,
        "yoda": 2,
        "no-shadow": 2,
        "no-undef-init": 2,
        "no-use-before-define": [2, {"functions": false}],
        "camelcase": 2,
        "new-cap": [2, {
            "capIsNew": false}],
        "new-parens": 2,
        "no-multi-assign": 2,
        "no-nested-ternary": 2,
        "no-underscore-dangle": ["error", { "allow": [
            "_calledComponentWillUnmount",
            "__REDUX_DEVTOOLS_EXTENSION_COMPOSE__",
            "_leaflet_id"
        ] }],
        "no-unneeded-ternary": 2,
        "one-var": [2, "never"],
        "constructor-super": 2,
        "no-this-before-super": 2,
        "no-useless-computed-key": 2,
        "no-useless-constructor": 2,
        "no-useless-rename": 2,
        "object-shorthand": 2,
        "prefer-const": 2,
        "prefer-template": 2,
        "no-debugger": 1,
        "no-cond-assign": [2, "always"],
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-extra-boolean-cast": 0,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-unexpected-multiline": 0,
        "block-scoped-var": 2,
        "default-case": 2,
        "guard-for-in": 2,
        "no-alert": 1,
        "no-case-declarations": 2,
        "no-div-regex": 0,
        "no-eq-null": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-redeclare": 2,
        "no-unused-expressions": [
          2,
          {
            "allowShortCircuit": true,
            "allowTernary": false
          }
        ],
        "no-with": 2,
        "radix": 2,
        "wrap-iife": [2, "outside"],
        "arrow-parens": 0,
        "arrow-spacing": [
          2,
          {
            "before": true,
            "after": true
          }
        ],
        "no-class-assign": 2,
        "no-const-assign": 2,
        "prefer-arrow-callback": 2,
        "prefer-spread": 2,
        "strict": [2, "never"],
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "array-bracket-spacing": [
          2,
          "never",
          {
            "singleValue": false,
           "objectsInArrays": false,
              "arraysInArrays": false
            }
        ],
        "newline-after-var": 0,
        "brace-style": [2, "1tbs"],
        "comma-spacing": [
         2,
            {
              "before": false,
              "after": true
            }
        ],
        "comma-style": [2, "last"],
        "computed-property-spacing": [2, "never"],
        "eol-last": 2,
        "func-names": 1,
        "func-style": [
          2,
          "declaration",
          {
            "allowArrowFunctions": true
          }
        ],
        "linebreak-style": 0,
        "max-len": [
          2,
           200,
           4,
          {
            "ignoreUrls": true,
            "ignoreComments": false
          }
        ],
        "no-array-constructor": 2,
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": [
          2,
           {
            "max": 2,
            "maxEOF": 1
          }
        ],
        "no-new-object": 2,
        "no-spaced-func": 2,
        "object-curly-spacing": [2, "always"],
        "padded-blocks": [2, "never"],
        "semi-spacing": [
          2,
            {
             "before": false,
             "after": true
            }
        ],
        "keyword-spacing": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": [
          2,
          {
            "anonymous": "always",
            "named": "never"
          }
        ],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-unary-ops": [
          2,
          {
            "words": true,
            "nonwords": false
         }
        ],
        "spaced-comment": [
          2,
          "always",
           {
             "exceptions": ["-", "+"],
             "markers": ["=", "!"]
            }
        ],
        "comma-dangle": ["error", "never"],
        "react/button-has-type": "error",
        "react/display-name": "error",
        "react/default-props-match-prop-types": "error",
        "react/forbid-prop-types": ["error", {"forbid": ["any", "array"]}],
        "react/jsx-max-depth": ["error", {"max": 8}],
        "react/no-access-state-in-setstate": "error",
        "react/no-children-prop": "error",
        "react/no-danger": "error",
        "react/no-deprecated": "error",
        "react/no-direct-mutation-state": "error",
        "react/no-find-dom-node": "error",
        "react/no-multi-comp": ["error", {"ignoreStateless": true}],
        "react/no-redundant-should-component-update": "error",
        "react/no-typos": "error",
        "react/no-string-refs": "error",
        "react/no-this-in-sfc": "error",
        "react/no-unescaped-entities": "error",
        "react/no-unknown-property": "error",
        "react/no-unsafe": "error",
        "react/no-unused-prop-types": "error",
        "react/no-unused-state": "error",
        "react/prefer-es6-class": "error",
        "react/prefer-read-only-props": "error",
        "react/prefer-stateless-function": ["error", {"ignorePureComponents": true}],
        "react/react-in-jsx-scope": "error",
        "react/require-render-return": "error",
        "react/self-closing-comp": "error",
        "react/sort-comp": "error",
        "react/style-prop-object": "error",
        "react/void-dom-elements-no-children": "error",
        "react/jsx-boolean-value": "error",
        "react/jsx-filename-extension": ["error", {"extensions": [".js", ".tsx"]}],
        "react/jsx-fragments": ["error", "syntax"],
        "react/jsx-key": "error",
        "react/jsx-no-bind": ["error", {"allowArrowFunctions": true, "allowFunctions": true}],
        "react/jsx-no-duplicate-props": "error",
        "react/jsx-no-target-blank": "error",
        "react/jsx-pascal-case": "error",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "react/jsx-no-undef": "error",
        "more/no-filter-instead-of-find": "error",
        "more/no-void-map": "error",
        "no-duplicate-imports": "error",
        "jest/no-disabled-tests": "error",
        "jest/consistent-test-it": ["error", {"fn": "it"}],
        "jest/prefer-spy-on": "error",
        "jest/valid-describe-callback": "error",
        "jest/valid-expect": "error",
        "jest/no-identical-title": "error",
        "jest/no-focused-tests": "error",
        "promise/no-return-wrap": "error",
        "promise/param-names": "error",
        "promise/catch-or-return": "error",
        "import/no-internal-modules": [ "error", { "forbid": ["/@material-ui/core/**"]}],
        "import/no-self-import": "error",
        "import/no-cycle": "error",
        "import/no-absolute-path": "error",
        "import/no-webpack-loader-syntax": "error",
        "import/order": [
          "error",
          {
            "groups": [
              ["builtin", "external"],
              ["internal", "index", "parent", "sibling"]
            ]
          }
        ]
    },
    "overrides": [
        {
            "files": "*.test.js",
            "rules": {
                "no-magic-numbers": "off",
                "camelcase": "off",
                "react/prefer-stateless-function": "off",
                "prettier/prettier": "off",
                "jest/no-identical-title": "off"
            }
        }
    ]
}
